{
	"manifest_version" : 2,
	"name" : "jeforth.3ce",
	"description" : "jeforth.3we for Chrome Extension",
	"version" : "1.0",
	"browser_action" : {
		"default_title": "Popup of jeforth.3ce",
		"default_icon" : "3ce/fig.tw16x16.png",
		"default_popup" : "jeforth.3ce.html"
	},
	"content_scripts": [{
		"matches":["http://*/*", "https://*/*"],
		"js": ["3ce/content_script.js"]
	}],
	"content_security_policy" : "script-src 'self' 'unsafe-eval'; object-src 'self'",
	"permissions" : [
		"tabs", "notifications", "http://*/*", "https://*/*"
	]
}


