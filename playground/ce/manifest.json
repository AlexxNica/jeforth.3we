{
  "manifest_version": 2,
  "name": "project-k baby",
  "description": "jeforth proejct-k baby example",
  "version": "1.0",
  "browser_action": {
    "default_title": "project-k baby default title",
    "default_icon": "fig.tw16x16.png",
    "default_popup": "sample.html"
  },
  "content_scripts": [ {
    "js": [ "jquery-1.11.2.js", "background.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",  
  "permissions": [
    "activeTab","http://*/*","https://*/*"
  ]
}
